{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": "sts:AssumeRole",
        "Principal": {
          "Service": "lambda.amazonaws.com"
        }
      },{
        "Effect": "Allow",
        "Action": "logs:CreateLogStream",
        "Resource": "arn:aws:logs:us-east-2:627948436154:log-group:/aws/lambda/rancor-ami:*"
      },{
        "Effect": "Allow",
        "Action": "logs:PutLogEvents",
        "Resource": "arn:aws:logs:us-east-2:627948436154:log-group:/aws/lambda/rancor-ami:*:*"
      },{
        "Effect": "Allow",
        "Action": "ec2:*",
        "Resource": "*"
      },{
        "Effect": "Allow",
        "Action": "SNS:Publish",
        "Resource": "arn:aws:sns:us-east-2:627948436154:rancor-sns"
      }
    ]
}