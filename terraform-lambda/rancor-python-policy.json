{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": "sts:AssumeRole",
        "Principal": {
          "Service": "lambda.amazonaws.com"
        }
      },{
        "Effect": "Allow",
        "Action": "logs:CreateLogStream",
        "Resource": "arn:aws:logs:us-east-1:627948436154:log-group:/aws/lambda/rancor-terminate-interrupt:*"
      },{
        "Effect": "Allow",
        "Action": "logs:PutLogEvents",
        "Resource": "arn:aws:logs:us-east-1:627948436154:log-group:/aws/lambda/rancor-terminate-interrupt:*:*"
      }
    ]
  }